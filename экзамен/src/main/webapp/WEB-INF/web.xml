<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>lab2Servlet</servlet-name>
        <servlet-class>IP11lab2.lab2Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Lab5Servlet</servlet-name>
        <servlet-class>TheoryOfAlgorithms.Borisyuk.Lab5.Lab5Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet</servlet-name>
        <servlet-class>Ir11exam.Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>lab2Servlet</servlet-name>
        <url-pattern>/lab2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Lab5Servlet</servlet-name>
        <url-pattern>/lab5</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet</servlet-name>
        <url-pattern>/formula</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
